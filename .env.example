# ==============================================
# NRJSoft Parking App - Environment Variables
# ==============================================
# Copy this file to .env.development, .env.staging, or .env.production
# and fill in the appropriate values.
#
# NEVER commit actual secrets to version control!
# ==============================================

# API Configuration
API_BASE_URL=https://api.nrjsoft.com
API_TIMEOUT=30000

# Google Maps
GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# Stripe Payment
STRIPE_PUBLISHABLE_KEY=pk_test_xxxxxxxxxxxxxxxxxx
STRIPE_MERCHANT_ID=merchant.com.nrjsoft.parking

# Google Sign-In
GOOGLE_WEB_CLIENT_ID=xxxxxxxxxxxx.apps.googleusercontent.com
GOOGLE_IOS_CLIENT_ID=xxxxxxxxxxxx.apps.googleusercontent.com

# Firebase
FIREBASE_PROJECT_ID=nrjsoft-parking
FIREBASE_APP_ID_IOS=1:xxxxxxxxxxxx:ios:xxxxxxxxxxxx
FIREBASE_APP_ID_ANDROID=1:xxxxxxxxxxxx:android:xxxxxxxxxxxx

# Sentry (Error Tracking)
SENTRY_DSN=https://xxxxxxxxxxxx@sentry.io/xxxxxxxxxxxx

# Feature Flags
ENABLE_WALLET=true
ENABLE_SUBSCRIPTIONS=true
ENABLE_SOCIAL_LOGIN=true
ENABLE_BIOMETRIC_AUTH=true
ENABLE_ANALYTICS=true
ENABLE_CRASH_REPORTING=true

# App Config
APP_NAME=NRJ Parking
APP_VERSION=1.0.0
SUPPORT_EMAIL=support@nrjsoft.com
SUPPORT_PHONE=+49 800 223 4455

# Debug (Development only)
DEBUG_MODE=false
ENABLE_FLIPPER=true
LOG_LEVEL=debug
